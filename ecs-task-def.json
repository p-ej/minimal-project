{
  "family": "role-nestjs-second",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "role-nestjs",
  "taskRoleArn": "role-nestjs",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "REPLACE_IMAGE_URI",
      "portMappings": [{ 
        "containerPort": 80, 
        "protocol": "tcp",
        "name": "container-nestjs-80-tcp",
        "appProtocol": "http"
        }],
      "essential": true
    }
  ]
}
